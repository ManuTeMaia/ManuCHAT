{
    "compilerOptions": {
        "outDir": "dist",
        "baseUrl": "./src/",
        "declaration": false,
        "target": "es2016",
        "module": "CommonJS",
        "esModuleInterop": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
     },
      
    "exclude": ["node_modules", "server.ts", "dist"]
}